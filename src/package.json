{
  "name": "ulbackpacking",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "jest"
  },
  "dependencies": {
    "@nuxtjs/vuetify": "^0.5.5",
    "cookieparser": "^0.1.0",
    "firebase": "^6.1.0",
    "js-cookie": "^2.2.0",
    "jwt-decode": "^2.2.0",
    "nuxt": "^2.3.4",
    "nuxt-fire": "^1.4.1",
    "vue-feather-icons": "^4.21.0",
    "vue-input-autowidth": "^1.0.5",
    "vue-tour": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "convert-units": "^2.3.4",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-nuxt": "^0.3.0",
    "eslint-plugin-vue": "^5.1.0",
    "jest": "^24.8.0",
    "jest-serializer-vue": "^2.0.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-jest": "^3.0.4"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "watchman": false,
    "moduleNameMapper": {
      "^~/(.*)$": "<rootDir>/src/$1",
      "^~~/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".*\\.(vue)$": "vue-jest"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/src/components/**/*.vue",
      "<rootDir>/src/pages/*.vue"
    ],
    "testURL": "http://localhost/"
  }
}
